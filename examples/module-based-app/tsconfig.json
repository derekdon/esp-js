{
  "compilerOptions": {
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "allowJs": false,
    "jsx": "react",
    "experimentalDecorators":true,
    "target": "es5",
    "module": "commonjs",
    "noEmit": true,
    "typeRoots" : ["./typings", "./node_modules/@types"],
    "outDir" : "./.tsbuild"
  },
  "include": [
    "src/**/*"
  ]
}